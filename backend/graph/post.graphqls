scalar Upload
scalar Byte

input NewPost {
  text: String!
  user_id: String!
  attachment_link: String!
}

type Post {
  id: ID!
  text: String!
  AttachmentLink: String!
  User: User!
}

extend type Query {
  posts: [Post!]!
}

extend type Mutation {
  # createPost(input: NewPost!, file: Upload!): String!
  createPost(input: NewPost!): String!
}
